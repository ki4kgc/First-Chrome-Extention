{
  "manifest_version": 2,

  "name": "First Chrome Extension",
  "description": "This extension demonstrates a browser action context script.",
  "version": "2.0",

  "permissions": [
    "tabs", "http://*/*"
  ],

   "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]

}